{
  "name": "ficat-vue",
  "version": "1.0.0",
  "description": "Nuxt-Vue.js Ficat version",
  "author": "Lakshamana",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=dev.local nodemon server/index.js --watch server",
    "debug": "cross-env NODE_ENV=development DEBUG=koa* nodemon --harmony server/index.js --watch server",
    "debug-insp": "cross-env NODE_ENV=dev.local nodemon --inspect server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:nuxt": "nuxt start",
    "generate": "nuxt generate",
    "seed:devmake": "cross-env NODE_ENV=development knex seed:make",
    "seed:testmake": "cross-env NODE_ENV=test knex seed:make",
    "seed:prod": "cross-env NODE_ENV=production knex seed:run",
    "seed": "knex seed:run",
    "migrate:make": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "migrate:prod": "cross-env NODE_ENV=production knex migrate:latest",
    "rollback": "knex migrate:up",
    "makedb": "if test -f dev.sqlite3; then rm dev.sqlite3; fi; npm run migrate && npm run seed",
    "makedb:test": "if test -f test.sqlite3; then rm test.sqlite3; fi; npm run migrate && npm run seed",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "test": "jest --maxWorkers=4 --force-exit",
    "ci": "npm test && npm run lint"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/recaptcha": "^0.6.2",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "axios": "^0.21.1",
    "bcrypt": "^3.0.6",
    "bookshelf": "^0.15.1",
    "bookshelf-secure-password": "^4.0.0",
    "buefy": "^0.9.10",
    "chart.js": "^2.9.4",
    "dotenv": "^8.0.0",
    "googleapis": "^83.0.0",
    "hooper": "^0.3.4",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.3",
    "koa": "^2.6.2",
    "koa-body": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-unless": "^1.0.7",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "nuxt": "^2.15.7",
    "nuxt-fontawesome": "^0.4.0",
    "p-debounce": "^2.1.0",
    "pdfkit": "^0.10.0",
    "sqlite3": "^4.1.0",
    "vue-cookies": "^1.5.13",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "node-sass": "^6.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sass-loader": "^10",
    "vue-jest": "^4.0.1"
  }
}
